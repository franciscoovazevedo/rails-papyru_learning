<%= simple_form_for @teacher_subject do |f| %>
  <%= f.input :subject_id, collection: Subject.all %>
  <%= f.input :description%>
  <%= f.input :price%>
  <%= f.input :begin, as: :date, html5:  true%>
  <%= f.input :end, as: :date, html5:  true%>

  <%= f.submit "Submit", class: "btn btn-success" %>
<% end -%>

